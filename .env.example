# MongoDB connection string (Atlas or local)
# Example Atlas: mongodb+srv://USER:PASSWORD@cluster0.xxxxx.mongodb.net/oncoliving?retryWrites=true&w=majority
# Example local: mongodb://127.0.0.1:27017/oncoliving
MONGODB_URI=
# Optional: overrides db name extracted from URI
MONGODB_DB=oncoliving

# Application configuration
VITE_APP_ID=oncoliving-local
JWT_SECRET=replace-with-secure-secret
OAUTH_SERVER_URL=http://localhost:3001
OWNER_OPEN_ID=owner-local
BUILT_IN_FORGE_API_URL=
BUILT_IN_FORGE_API_KEY=
VITE_ANALYTICS_ENDPOINT=
VITE_ANALYTICS_WEBSITE_ID=
VITE_OAUTH_PORTAL_URL=http://localhost:3001
VITE_DEV_LOCAL_AUTH=true

# Server settings
PORT=3000
NODE_ENV=development
